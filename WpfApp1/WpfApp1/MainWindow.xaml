<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="950" Width="1800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="ButtonCreateStarterTable" Content="Create a Starter Table" Click="ButtonCreateStarterTable_Click" Width="200" Height="50" Margin="0,0,0,10"/>
            <Button x:Name="ButtonCreateManyToMany" Content="Create a Many-to-Many Table" Click="ButtonCreateManyToMany_Click" Width="200" Height="50" Margin="0,0,0,10"/>
        </StackPanel>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Margin="0,100,0,0"/>
    </Grid>
</Window>
